{
	<Readline>
	Memcheck:Leak
	match-leak-kinds: reachable
	...
	fun:readline
	...
}
{
	<Readline/AddHistory>
	Memcheck:Leak
	match-leak-kinds: reachable
	fun:malloc
	...
	fun:add_history
	...
}
{
	ignore_readline_teardown_leak
	Memcheck:Leak
	match-leak-kinds: definite
	fun:readline_internal_teardown
	fun:xmalloc
}
